Windows Registry Editor Version 5.00

[HKEY_CURRENT_USER\Software\Classes\*\shell\nvim]
@="Open in New NVim Window"

[HKEY_CURRENT_USER\Software\Classes\*\shell\nvim\command]
@="\"C:\\Program Files\\Neovim\\bin\\nvim-qt.exe\" \"%1\""

[HKEY_CURRENT_USER\Software\Classes\*\shell\nvim-server]
@="Open in Unified NVim Window"

[HKEY_CURRENT_USER\Software\Classes\*\shell\nvim-server\command]
@="cmd.exe /Q /C \"start /MIN nvr.exe --servername 127.0.0.1:65000 -s --nostart --remote \"\"%1\"\"\""
